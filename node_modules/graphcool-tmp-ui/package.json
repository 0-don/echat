{
  "name": "graphcool-tmp-ui",
  "version": "0.0.11",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc && babel lib -d lib && rm -rf lib/*.jsx",
    "lint": "tslint \"src/**/*.ts{,x}\"",
    "test": "yarn lint && yarn build",
    "prettier": "prettier --single-quote --no-semi --trailing-comma all --write '*.{js,ts,tsx}' 'src/**/*.{js,ts,tsx}'",
    "prepublish": "yarn build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --single-quote --no-semi --trailing-comma all --write",
      "tslint",
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "repository": "https://github.com/graphcool/graphcool-ui.git",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "graphcool-styles": "^0.1.31",
    "react-modal": "^1.6.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/react": "^15.0.39",
    "@types/react-dom": "^15.5.1",
    "@types/react-modal": "^1.6.8",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-inherit": "git+https://github.com/timsuchanek/postcss-inherit#build3",
    "postcss-simple-vars": "^4.0.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-jsx": "^0.5.2",
    "styled-jsx-postcss": "git+https://github.com/timsuchanek/styled-jsx-postcss#build3",
    "tslint": "^5.5.0",
    "tslint-graphcool-frontend": "^0.0.3",
    "typescript": "^2.4.2"
  }
}
